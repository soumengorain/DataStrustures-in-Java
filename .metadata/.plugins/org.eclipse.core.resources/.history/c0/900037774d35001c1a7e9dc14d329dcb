package JavaBasics;

public class Reverse {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String ar="    Hi, my     name     is soumen gorain      ".replaceAll("( +)"," ").trim();
//		str.replaceAll("( +)"," ").trim()
		
		String[] str=ar.split(" ");
		
		for(int i=str.length-1;i>=0;i--)
			System.out.println(str[i]);
		
	}

}
