package JavaBasics;

import java.util.Scanner;

public class Practice {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int arr[]= {4,2,7,1,8,3,-5,0,12};
		int n=arr.length;
		String a="asvr";
		System.out.println();
		for(int i=0;i<n;i++)
		{
			int min=i;
			for(int j=i+1;j<n;j++)
			{
				if(arr[min]>arr[j])
					min=j;
				
			}
			int temp=arr[i];
			arr[i]=arr[min];
			arr[min]=temp;
		}
		for(int x:arr)
			System.out.print(x + " ");
	}

}
